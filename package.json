{
  "name": "actions-test",
  "private": true,
  "version": "1.3.0",
  "scripts": {
    "start": "cross-env APP_VARIANT=development expo start",
    "android": "cross-env APP_VARIANT=development expo run:android",
    "ios": "cross-env APP_VARIANT=development expo run:ios",
    "web": "cross-env APP_VARIANT=development expo start --web",
    "experiment": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/experiment.ts",
    "lint": "eslint . --config .eslintrc.production.js --max-warnings 0",
    "lint:fix": "eslint . --config .eslintrc.production.js --fix --max-warnings 0",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "test:ci": "vitest --run",
    "integration": "vitest --config=vitest.integration.config.ts",
    "coverage": "vitest --run --coverage related",
    "coverage:all": "vitest --run --coverage",
    "eas-build-pre-install": "npm i -g pnpm@8.11.0",
    "build:development": "eas build --platform android --profile development",
    "build:preview": "eas build --platform android --profile preview",
    "build:staging": "eas build --platform android --profile staging",
    "build:production": "eas build --platform android --profile production",
    "build:development:local": "eas build --local --platform android --profile development-local",
    "build:preview:local": "eas build --local --platform android --profile preview-local",
    "build:staging:local": "eas build --local --platform android --profile staging-local",
    "build:production:local": "eas build --local --platform android --profile production-local",
    "update:preview": "cross-env APP_VARIANT=preview eas update --auto --platform android --channel preview",
    "update:staging": "cross-env APP_VARIANT=staging eas update --auto --platform android --channel staging",
    "update:production": "cross-env APP_VARIANT=production eas update --auto --platform android --channel production"
  },
  "dependencies": {
    "@effect/platform": "0.33.0",
    "@effect/schema": "0.52.0",
    "@effect/typeclass": "0.18.0",
    "@expo/vector-icons": "13.0.0",
    "@fp-ts/optic": "0.19.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "assert": "^2.1.0",
    "effect": "2.0.0-next.59",
    "expo": "^49.0.21",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.12",
    "expo-navigation-bar": "~2.3.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.18",
    "fast-text-encoding": "^1.0.6",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1",
    "react-native-url-polyfill": "^2.0.0",
    "stream": "^0.0.2"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@babel/core": "^7.23.5",
    "@changesets/cli": "^2.27.1",
    "@cspell/dict-pt-br": "^2.3.0",
    "@expo/config-types": "49.0.0",
    "@types/benchmark": "^2.1.5",
    "@types/react": "~18.2.39",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "^1.0.1",
    "benchmark": "^2.1.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "fast-check": "^3.14.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsutils": "^3.21.0",
    "typescript": "~5.3.2",
    "vite": "^5.0.5",
    "vitest": "^1.0.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/preset-env",
        "@types/node",
        "expo-modules-autolinking"
      ]
    }
  }
}
